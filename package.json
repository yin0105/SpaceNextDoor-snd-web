    "@svgr/webpack": "^5.5.0",
    "@typeform/embed-react": "^1.0.5",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.13",
    "axios": "^0.21.1",
    "chart.js": "^3.0.2",
    "clevertap-react": "^1.0.0",
    "clsx": "^1.1.1",
    "date-fns": "2.8.1",
    "dayjs": "^1.10.4",
    "graphql": "^15.4.0",
    "joi": "^17.4.2",
    "json2csv": "^5.0.6",
    "keen-slider": "^5.4.1",
    "libphonenumber-js": "^1.9.11",
