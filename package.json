{
  "name": "space-next-door",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "predev": "node getStaticData.ts",
    "dev": "next dev -p 3000",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev -p 3000",
    "prebuild": "node getStaticData.ts",
    "build": "next build",
    "export": "next export",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "buildstatic": "next build && next export",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "eslint . --ext .tsx",
    "lint:export": "eslint . --ext .tsx -f checkstyle -o checkstyle-result.xml",
    "lint:fix": "eslint --fix . --ext .tsx,.ts",
    "start:codegen": "apollo client:codegen --no-addTypename --globalTypesFile=src/typings/graphql.types.ts --target=typescript --watch --includes=src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@date-io/date-fns": "1.3.13",
